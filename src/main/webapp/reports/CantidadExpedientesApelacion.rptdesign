            <xml-property name="queryText"><![CDATA[SELECT  DISTINCT p.idprocedimiento,p.nombre, 
fnc_proyapel_pr(p.idprocedimiento) as PROY_APELACION_PR , 
fnc_proyapel_r(p.idprocedimiento) AS PROY_APELACION_TR
FROM  expedientesas ex  , procedimiento p , expediente e
WHERE ex.idprocedimiento= p.idprocedimiento and
e.idexpediente = ex.idexpediente and
(
fnc_proyapel_pr(p.idprocedimiento)<>0 or
fnc_proyapel_r(p.idprocedimiento)<>0
)]]></xml-property>
