            <xml-property name="queryText"><![CDATA[select DISTINCT exsas.analista,u.nombres  || ' ' || u.apellidos  as nombres,
  FNC_CANT_OFI_AG(u.idusuario) as OficiosGenerados,
  FNC_CANT_OFI_AP(u.idusuario)   as OficiosPendientes 
  from expediente ex, expedientesas exsas,concesionario co, usuario u
  where ex.concesionario = co.idconcesionario and 
        ex.idexpediente = exsas.idexpediente and 
        exsas.analista = u.idusuario
]]></xml-property>
