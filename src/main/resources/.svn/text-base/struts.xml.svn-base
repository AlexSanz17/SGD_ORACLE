      </interceptors>

      <global-results>
         <result name="forwardToURL">forwardToURL.jsp</result>
         <result name="202">error/errorAlfresco.html</result>
         <result name="303">error/errorIntalio.html</result>
         <result name="true">pages/util/true.jsp</result>
         <result name="false">pages/util/false.jsp</result>
         <result name="error">error.html</result>
      </global-results>

